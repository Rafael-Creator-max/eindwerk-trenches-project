                {
                    "name": "Register a new user",
                    "request": {
                        "url": {
                            "host": "{{baseUrl}}",
                            "path": "api\/register",
                            "query": [],
                            "raw": "{{baseUrl}}\/api\/register"
                        },
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application\/json"
                            },
                            {
                                "key": "Accept",
                                "value": "application\/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\"name\":\"John Doe\",\"email\":\"user@example.com\",\"password\":\"your-secure-password\",\"password_confirmation\":\"your-secure-password\"}"
